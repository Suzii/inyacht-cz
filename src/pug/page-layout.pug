extends html-layout
append styles
    // Custom styles for this template
    link(href="./css/bootstrap/dashboard.css", rel="stylesheet")
    link(href="./css/bootstrap/index.css", rel="stylesheet")

block body
    nav.navbar.navbar-default.navbar-fixed-top
        .container
            .navbar-header
                button.navbar-toggle.collapsed(type="button", data-toggle="collapse", data-target="#navbar", aria-expanded="false", aria-controls="navbar")
                    span.sr-only Toggle navigationn
                    span.icon-bar
                    span.icon-bar
                    span.icon-bar
                a.navbar-brand(href="index.html")
                    img(alt="inYacht", src="img/logo-small.png")
            #navbar.navbar-collapse.collapse
                ul.nav.navbar-nav.navbar-right
                    li
                        a(href="./index.html") O nás
                    li
                        a(href="#") Online vyhledávač
                    li(class="dropdown")
                        a(href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false")
                            | Námořní jachting
                            span.caret

                        ul.dropdown-menu
                            li
                                a(href="#") Časté otázky
                            li
                                a(href="#") Novinky
                            li
                                a(href="#") Oblíben destinace
                            li
                                a(href="#") Počasí na moři
                    li
                        a(href="#") Kapitánske kurzy
                    li
                        a(href="#") Kontakt

    block content
